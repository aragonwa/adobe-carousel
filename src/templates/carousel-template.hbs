<div id="carousel" tabindex="1">
    <ul id="slides">
    {{#each this}}
        <li class="slide{{#if @first}} active{{/if}}">
        <img width="640" height="480" src="{{formatSrcUrl this}}" />
        <p class="slide-title"><span>{{title}}</span></p>
        </li>
    {{/each}}
    </ul>
    <div id="controls">
    <div id="start-stop" tabindex="2">
        <span class="fa fa-pause fa-lg" ><span class="sr-only">Pause</span></span>
        <span class="fa fa-play fa-lg"><span class="sr-only">Play</span></span>
    </div>
    <span id="left" >
        <span class="fa-stack fa-lg" tabindex="3">
        <span class="fa fa-square fa-stack-2x" ></span>
        <span class="fa fa-chevron-left fa-stack-1x"></span>
        <span class="sr-only">Previous</span>
        </span>
    </span>
    <span id="right" >
        <span class="fa-stack fa-lg" tabindex="4">
        <span class="fa fa-square fa-stack-2x"></span>
        <span class="fa fa-chevron-right fa-stack-1x"></span>
        <span class="sr-only">Next</span>
        </span>
    </span>
    </div>
</div>
